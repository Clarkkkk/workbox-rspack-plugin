{
  "name": "@aaroon/workbox-rspack-plugin",
  "description": "An Rspack plugin to use workbox",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Aaron Zhou",
  "keywords": [
    "workbox",
    "rspack",
    "rsbuild",
    "workbox-webpack-plugin",
    "pwa",
    "service worker",
    "manifest"
  ],
  "homepage": "https://github.com/Clarkkkk/workbox-rspack-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clarkkkk/workbox-rspack-plugin"
  },
  "bugs": {
    "url": "https://github.com/Clarkkkk/workbox-rspack-plugin/issues"
  },
  "packageManager": "pnpm@8.6.3",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "analyze": "vite-bundle-visualizer",
    "test": "vitest",
    "test:update": "vitest --update",
    "test:run": "vitest --run --changed",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "format": "eslint --fix --cache src/**/*.{vue,ts}",
    "prepare": "husky install",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0",
    "pathe": "^1.1.2",
    "pretty-bytes": "^6.1.1",
    "workbox-build": "^7.0.0"
  },
  "peerDependencies": {
    "@rspack/core": ">=0.5.6"
  },
  "devDependencies": {
    "@rspack/cli": "^0.5.6",
    "@rspack/core": "^0.5.6",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "commit-and-tag-version": "^12.2.0",
    "conventional-changelog-aaron-preset": "^0.2.3",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "memfs": "^4.7.7",
    "module-alias": "^2.2.3",
    "prettier": "^3.2.5",
    "rollup-plugin-node-externals": "^7.0.1",
    "service-worker-mock": "^2.0.5",
    "tempy": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-bundle-visualizer": "^1.0.1",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "webpack": "^5.90.3",
    "workbox-precaching": "^7.0.0"
  }
}
